{
    "name": "thedevs-cz/wboost",
    "type": "project",
    "description": "",
    "authors": [
        {
            "name": "Jan MikeÅ¡",
            "email": "j.mikes@me.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-libxml": "*",
        "ext-redis": "*",
        "ext-simplexml": "*",
        "ext-uuid": "*",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "doctrine/orm": "^3.0",
        "dompdf/php-font-lib": "^1.0",
        "endroid/qr-code": "^6.0",
        "jeroendesloovere/vcard": "^1.7",
        "jetbrains/phpstorm-attributes": "^1.0",
        "knplabs/knp-time-bundle": "^2.2",
        "league/flysystem-async-aws-s3": "^3.0",
        "liip/imagine-bundle": "^2.12",
        "lustmored/flysystem-v2-simple-cache-adapter": "^0.3.0",
        "monolog/monolog": "^3.0",
        "nelmio/cors-bundle": "^2.4",
        "nette/utils": "^4.0",
        "oneup/flysystem-bundle": "^4.9",
        "psr-discovery/cache-implementations": "^1.1",
        "psr-discovery/container-implementations": "^1.1",
        "psr-discovery/event-dispatcher-implementations": "^1.1",
        "psr-discovery/http-client-implementations": "^1.1",
        "ramsey/uuid": "^4.2",
        "ramsey/uuid-doctrine": "^2.0",
        "sentry/sentry-symfony": "^5.0",
        "symfony/asset": "^8.0",
        "symfony/asset-mapper": "^8.0",
        "symfony/config": "^8.0",
        "symfony/console": "^8.0",
        "symfony/dependency-injection": "^8.0",
        "symfony/doctrine-messenger": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/expression-language": "^8.0",
        "symfony/flex": "^2.1",
        "symfony/form": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/http-client": "^8.0",
        "symfony/http-client-contracts": "^3.4",
        "symfony/http-kernel": "^8.0",
        "symfony/intl": "^8.0",
        "symfony/mailer": "^8.0",
        "symfony/messenger": "^8.0",
        "symfony/mime": "^8.0",
        "symfony/monolog-bundle": "^4.0",
        "symfony/process": "^8.0",
        "symfony/property-access": "^8.0",
        "symfony/property-info": "^8.0",
        "symfony/runtime": "^8.0",
        "symfony/security-bundle": "^8.0",
        "symfony/security-csrf": "^8.0",
        "symfony/serializer": "^8.0",
        "symfony/string": "^8.0",
        "symfony/translation": "^8.0",
        "symfony/twig-bundle": "^8.0",
        "symfony/ux-live-component": "^2.19",
        "symfony/ux-turbo": "^2.18",
        "symfony/validator": "^8.0",
        "symfony/web-link": "^8.0",
        "symfony/yaml": "^8.0",
        "twig/cssinliner-extra": "^3.10",
        "twig/extra-bundle": "^3.10",
        "twig/inky-extra": "^3.10",
        "twig/intl-extra": "^3.8",
        "twig/string-extra": "^3.7",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpunit/phpunit": "^11.0",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^8.0",
        "symfony/css-selector": "^8.0",
        "symfony/debug-bundle": "^8.0",
        "symfony/web-profiler-bundle": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "WBoost\\Web\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBoost\\Web\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan --memory-limit=-1 analyse",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "8.*",
            "docker": false
        }
    },
    "replace": {
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-uuid": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": false
        }
    }
}
